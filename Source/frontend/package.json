{
  "name": "alfred",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run config && ng serve",
    "build": "npm run config && ng build",
    "config": "npx ts-node src/environments/set-env.ts",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "playwright test",
    "e2e:aspire": "cross-env USE_ASPIRE=true playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:chromium": "playwright test --project=chromium",
    "e2e:report": "playwright show-report"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.3",
    "@angular/build": "^21.0.3",
    "@angular/cdk": "^21.0.3",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/material": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@angular/service-worker": "^21.0.3",
    "@tailwindcss/postcss": "^4.1.18",
    "colors": "^1.4.0",
    "dotenv": "^17.2.3",
    "rxjs": "~7.8.2",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.8.1",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.3",
    "@playwright/test": "^1.57.0",
    "@types/jasmine": "~5.1.13",
    "@types/node": "^25.0.1",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.1.0",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "typescript": "~5.9.3"
  }
}
